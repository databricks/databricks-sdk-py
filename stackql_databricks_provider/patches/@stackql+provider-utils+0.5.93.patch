+/**
+ * Formats a value using template syntax based on the field type.
+ * - Strings: "{{ name }}"
+ * - Booleans/numbers/integers: {{ name }}
+ */
+function formatTemplateValue(name, type) {
+    if (type === 'boolean' || type === 'number' || type === 'integer') {
+        return '{{ ' + name + ' }}';
+    }
+    return '"{{ ' + name + ' }}"';
+}
